// Place your settings in this file to overwrite default and user settings.
{
    "[python]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "python.linting.flake8Args": [
        // Match what black does.
        "--max-line-length=88"
    ],
}